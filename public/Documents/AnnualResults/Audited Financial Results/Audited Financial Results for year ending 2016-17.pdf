                }
                else {
                    $('.case').prop('checked', false);
                }
            });


            $(function () {
                $('#btnSelectScript').click(function () {
                    //var val = [];
                    var ValScripSelcect = '';
                    var txtScript;
                    $(':checkbox:checked').each(function (i) {
                        //  val[i] = $(this).val();  
                        if (i == 0) {
                            ValScripSelcect = $(this).val();
                        }
                        else {
                            ValScripSelcect = ValScripSelcect + "," + $(this).val();
                        }
                    });
                    txtScript = ValScripSelcect.replace('on,', '');
                    $('#txtSelectedScript').text(txtScript);
                    return false;
                });
            });
        });
    </script>

