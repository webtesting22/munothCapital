        function ValidatePANMaster() {
            if (document.getElementById('ctl00_txtPAN').value != "") {
                var panVal = document.getElementById('ctl00_txtPAN').value
                var regpan = /^([a-zA-Z]){5}([0-9]){4}([a-zA-Z]){1}?$/;

                if (regpan.test(panVal) != true) {
                    $('#ctl00_lblError').text('Invalid PAN No.')
                    return false;
                    
                }
                else {
                    $('#Error').hide();
                    $('#ctl00_lblError').text('');
                }
            }
           
            
                
           
            else {
     
                $('#ctl00_lblError').text('Enter PAN No.');
                document.getElementById('ctl00_txtPAN').focus();
                return false;
            }

            
        }
